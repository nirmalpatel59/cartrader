generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Listing {
  id          Int    @id @default(autoincrement())
  make        String
  model       String
  year        Int
  miles       Int
  price       Int
  city        String
  seats       Int
  features    String
  description String
  image       String
  userId      String
}
